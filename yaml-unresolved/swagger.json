swagger: '2.0'
info:
  version: '3.0.8'
  title: 'Netverify Web Implementation'
  description: 'This is a reference manual and configuration guide for the Netverify Web product. It illustrates how to embed Netverify into your web page, use Netverify redirect, and implement the performNetverify API.'
securityDefinitions:
  basicAuth:
    type: basic
paths:
  /initiateNetverify:
    post:
      security:
      - basicAuth: []
      description: Create a transaction for each user.
      produces:
        - json/application
      parameters:
        - name: merchantIdScanReference
          in: query
          description: Your reference for each scan must not contain sensitive data like PII (Personally Identifiable Information) or account login
          required: true
          type: string
        - in: header
          name: User-Agent
          type: string
          required: true
          description: e.g. MyCompany MyApp, change this to reflect your company.
        - in: query
          name: successUrl
          type: string
          required: true
          description: Redirect URL in case of success (for constraints see Success and error URLs). Mandatory if not specified in your Jumio portal.
        - in: query
          name: errorUrl
          type: string
          required: true
          description: Redirect URL in case of error (for constraints see Success and error URLs).  Mandatory if not specifed in your Jumio portal.
        - in: query
          name: enabledFields
          type: string
          required: false
          description: Defines fields which will be extracted during the ID verification. If a field is not listed in this parameter, it will not be processed for this transaction, regardless of customer portal settings. Note- Face match and Address extraction will not be processed unless enabled for your account. If you want to enable them, please contact your Customer Success Manager, or reach out to Jumio Support.
        - in: query
          name: authorizationTokenLifetime
          type: integer
          required: false
          description: Time in seconds until the authorization token expires.
        - in: query
          name: merchantReportingCriteria
          type: string
          required: false
          description: Your reporting criteria for each scan
        - in: query
          name: callbackUrl
          type: string
          required: false
          description: Callback URL for the confirmation after the verification is completed.
        - in: query
          name: locale
          type: string
          required: false
          description: Locale of the Netverify client.
          enum: [ bg, cs, da, de, el, en, es, es_MX, et, fi, fr, hu, it, ja, ko, lt, nl, no, pl, pt, pt_BR, ro, ru, sk, sv, tr, vi, zh_CN, zh_HK] 
        - in: query
          name: clientIp
          type: string
          required: false
          description: IP address of the client
        - in: query
          name: customerId
          type: string
          required: false
          description: Identification of the customer should not contain sensitive data like PII (Personally Identifiable Information) or account login
        - in: query
          name: firstName
          type: string
          required: false
          description: First name of the customer.
        - in: query
          name: lastName
          type: string
          required: false
          description: Last name of the customer.
        - in: query
          name: country
          type: string
          required: false
          description: Country of the customer.
        - in: query
          name: usState
          type: string
          required: false
          description: Possible values if idType = PASSPORT or ID_CARD
        - in: query
          name: expiry
          type: string
          required: false
          description: Date of expiry in the format YYYY-MM-DD
        - in: query
          name: number
          type: string
          required: false
          description: Identification number of the document
        - in: query
          name: dob
          type: string
          required: false
          description: Date of birth in the format YYYY-MM-DD
        - in: query
          name: idType
          type: string
          required: false
          description: PASSPORT, DRIVING_LICENSE, ID_CARD
          enum: [ PASSPORT, DRIVING_LICENSE, ID_CARD ]
        - in: query
          name: personalNumber
          type: string
          required: false
          description: Personal number of the document
        - in: query
          name: captureMethod
          type: string
          required: false
          enum: [ CAM, UPLOAD, ALL ]
          description: Capture method for this scan. Possible values- CAM, UPLOAD, ALL
        - in: query
          name: presetCountry
          type: string
          required: false
          description: Preset country and ID type to skip the Select country and ID type screen.
        - in: query
          name: presetIdType
          type: string
          required: false
          enum: [ PASSPORT, DRIVING_LICENSE, ID_CARD ]
          description: Preset country and ID type to skip the Select country and ID type screen.
      responses:
        200:
          description: Success
          schema:
            type: object
            properties:
              timestamp:
                type: string
                description: Timestamp of the response in the format YYYY-MM-DDThh:mm:ss.SSSZ
                example: 2020-08-16T10:27:29.494Z
              authorizationToken:
                type: string
                description: Authorization token, which is valid for the authorization token lifetime.
              jumioIdScanReference:
                type: string
                description: Jumio's reference number for each scan
#  /initiateNetverifyRedirect
#    post:
 #     description:
      
  /supportedIdTypes:
    get:
      description: Supported types of identification
      responses:
        200:
          description: Successful response
          schema:
            properties:
              supportedIdTypes:
                type: object
                properties:
                    countryName:
                      type: string
                      example: Austria
                    countryCode:
                      type: string
                      example: AT
                    idTypes:
                      type: object
                      properties:
                        idType:
                          type: string
                          enum: [PASSPORT, DRIVING_LICENSE, ID_CARD]
                          example: DRIVING_LICENSE
                        acquisitionConfig:
                          type: object
                          properties:
                            backside:
                              type: boolean
                              example: true
  /acceptedIdTypes:
    get:
      description:  Receive a JSON response including all accepted IDs, as specified in your Jumio portal settings.
      responses:
        200:
          description: Successful response
          schema:
            properties:
              acceptedIdTypes:
                type: object
                properties:
                    countryName:
                      type: string
                      example: Austria
                    countryCode:
                      type: string
                      example: AT
                    idTypes:
                      type: object
                      properties:
                        idType:
                          type: string
                          enum: [PASSPORT, DRIVING_LICENSE, ID_CARD]
                          example: DRIVING_LICENSE
                        acquisitionConfig:
                          type: object
                          properties:
                            backside:
                              type: boolean
                              example: true
                
